  "version": "0.2.0",
  "configurations": [
    {
      "preLaunchTask": "npm: build",
      "type": "node",
      "request": "launch",
      "name": "NestJs",
      "args": ["${workspaceFolder}/src/main.ts"],
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register",
        "-r",
        "tsconfig-paths/register"
      ],
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal",
      "protocol": "inspector"
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": ["9eb2998d-3fbb-4912-afa5-70aaedf9ad9e", "Entidade Gen√©rica"]
    }
  ]
}
